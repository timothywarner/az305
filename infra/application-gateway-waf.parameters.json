{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appGatewayName": {
      "value": "contoso-web"
    },
    "location": {
      "value": "eastus"
    },
    "vnetId": {
      "value": "/subscriptions/{subscription-id}/resourceGroups/rg-network/providers/Microsoft.Network/virtualNetworks/vnet-contoso-hub"
    },
    "appGatewaySubnetName": {
      "value": "AppGateway"
    },
    "skuTier": {
      "value": "WAF_v2"
    },
    "minCapacity": {
      "value": 2
    },
    "maxCapacity": {
      "value": 10
    },
    "enableWaf": {
      "value": true
    },
    "wafMode": {
      "value": "Prevention"
    },
    "wafRuleSetType": {
      "value": "OWASP"
    },
    "wafRuleSetVersion": {
      "value": "3.2"
    },
    "backendAddresses": {
      "value": [
        "app.contoso.com"
      ]
    },
    "backendPort": {
      "value": 443
    },
    "backendProtocol": {
      "value": "Https"
    },
    "enableHttpToHttpsRedirect": {
      "value": true
    },
    "keyVaultId": {
      "value": ""
    },
    "sslCertificateSecretName": {
      "value": ""
    },
    "logAnalyticsWorkspaceId": {
      "value": ""
    },
    "tags": {
      "value": {
        "environment": "production",
        "purpose": "application-gateway",
        "examObjective": "AZ-305-Networking"
      }
    }
  }
}
