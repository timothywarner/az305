    },
    "privateDnsZoneId": {
      "value": ""
    },
    "deployPrivateEndpoint": {
      "value": false
    },
    "keyVaultAdminPrincipalIds": {
      "value": []
    },
    "keyVaultSecretsUserPrincipalIds": {
      "value": []
    },
    "keyVaultCryptoUserPrincipalIds": {
      "value": []
    },
    "enableDiagnostics": {
      "value": false
    },
    "logAnalyticsWorkspaceId": {
      "value": ""
    },
    "tags": {
      "value": {
        "environment": "production",
        "purpose": "secrets-management",
        "examObjective": "AZ-305-SecretsManagement"
      }
    }
  }
