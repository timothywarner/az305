{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "initiativeName": {
      "value": "tag-governance-initiative"
    },
    "initiativeDisplayName": {
      "value": "Tag Governance Initiative"
    },
    "initiativeDescription": {
      "value": "Ensures resources have required tags for cost management, operational tracking, and compliance."
    },
    "policyCategory": {
      "value": "Tags"
    },
    "requiredTags": {
      "value": [
        "Environment",
        "CostCenter",
        "Owner",
        "Application",
        "DataClassification"
      ]
    },
    "defaultEnvironment": {
      "value": "Development"
    }
  }
}
